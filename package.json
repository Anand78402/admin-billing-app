{
  "name": "freeasy",
  "version": "1.0.0",
  "description": "Freeasy",
  "author": "Prem Baboo <p.baboo@huulke.com>",
  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "lint": "eslint --ext .js,.vue src",
    "lint:fix": "eslint --fix --ext .js,.vue src",
    "build": "vite build",
    "build:prod": "vite build --mode production",
    "build:staging": "vite build --mode staging",
    "start": "node server.js",
    "prod": "PORT=3002 NODE_ENV=prod forever start server.js",
    "prodrestart": "PORT=3002 NODE_ENV=prod forever restart server.js",
    "prodrestop": "PORT=3002 NODE_ENV=prod forever stop server.js",
    "staging": "PORT=3002 NODE_ENV=staging forever start server.js",
    "stagingrestart": "PORT=3002 NODE_ENV=staging forever restart server.js",
    "stagingrestop": "PORT=3002 NODE_ENV=staging forever stop server.js"
  },
  "dependencies": {
    "body-parser": "^1.20.2",
    "consolidate": "^1.0.3",
    "cookie-parser": "^1.4.6",
    "express": "^4.18.3",
    "fs": "^0.0.1-security",
    "http": "^0.0.1-security",
    "js-cookie": "^3.0.5",
    "moment": "^2.30.1",
    "underscore-node": "^0.1.2"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.12.16",
    "@babel/runtime": "^7.23.6",
    "@ckeditor/ckeditor5-build-classic": "^40.2.0",
    "@ckeditor/ckeditor5-vue": "^5.1.0",
    "@ckeditor/vite-plugin-ckeditor5": "^0.1.3",
    "@element-plus/icons-vue": "^2.3.1",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vueform/multiselect": "^2.6.6",
    "@vueform/slider": "^2.1.10",
    "@vuelidate/core": "^2.0.3",
    "@vuelidate/validators": "^2.0.4",
    "axios": "^1.6.2",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^29.7.0",
    "bootstrap": "^5.3.2",
    "bootstrap-vue-next": "^0.15.5",
    "core-js": "^3.34.0",
    "cross-env": "^7.0.3",
    "dropzone": "^6.0.0-beta.2",
    "echarts": "^5.4.3",
    "element-plus": "^2.7.5",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.0.3",
    "lint-staged": "^11.1.2",
    "maska": "^2.1.10",
    "metismenujs": "^1.4.0",
    "prettier": "^2.4.1",
    "sass": "^1.68.0",
    "sass-loader": "^13.3.2",
    "simplebar-vue": "^2.3.3",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-vue-components": "^0.26.0",
    "v-mask": "^2.3.0",
    "vite": "^4.5.1",
    "vite-plugin-babel": "^1.1.3",
    "vue": "^3.3.4",
    "vue-draggable-next": "^2.2.1",
    "vue-easy-lightbox": "^1.16.0",
    "vue-i18n": "^9.8.0",
    "vue-meta": "^2.4.0",
    "vue-router": "^4.2.5",
    "vue-slide-bar": "^1.2.0",
    "vue-switches": "^2.0.1",
    "vue3-count-to": "^1.1.2",
    "vue3-dropzone": "^2.2.0",
    "vue3-echarts": "^1.1.0",
    "vue3-form-wizard": "^0.2.1",
